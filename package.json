    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2"
  },
  "engines": {
    "node": ">= 10.15.0"
  },
  "standard": {
